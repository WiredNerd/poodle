{
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "black-formatter.args": [
        "--line-length=120"
    ],
    "isort.args": [
        "--profile=black",
        "--line-length=120"
    ],
    "python.testing.pytestArgs": [
        "-vv",
        "-s",
        "tests"
    ],
    "python.analysis.extraPaths": [
        "src",
        "tests",
        "example/src",
        "example/tests",
    ],
    "python.autoComplete.extraPaths": [
        "src",
        "example/src",
        "example/tests",
    ],
    "python.testing.pytestPath": "src",
    "cSpell.words": [
        "fromlist",
        "getaffinity",
        "keepends",
        "Mult",
        "PYTHONDONTWRITEBYTECODE",
        "redef",
        "wirednerd"
    ],
}